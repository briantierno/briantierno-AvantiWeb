<div id="lista-productos" class="container">
    <h3 id="nuestros productos" class="center red-text">Nuestros Productos</h3>
    <h5 id="canastitas">CANASTITAS <span class="precio">$ @ProductosService.ObtenerPrecioPorCategoria("canastita")</span> </h5>
    <div id="divisor" class="divider"></div>

    <div class="row">

        @{
            foreach (var p in Avanti_MVC.Models.ProductosService.ObtenerProductoPorCategoria("Canastita"))
            {

                <div class="col s6 m3 l2">
                    <div class="card">
                        <div class="card-image waves-block waves-light">
                            <img class="activator" src="images/Canastitas/@(p.Producto).jpg">
                            <span class="hide">@p.Precio</span>
                            <p class="hide">@p.Producto</p>
                            <a class="btn-floating halfway-fab waves-effect waves-light" data-id="@p.Id_producto"><i class="material-icons agregar-carrito">add</i></a>
                        </div>
                        <div class="card-content activator">
                            <span><i class="material-icons right">more_vert</i></span>
                            <p class="center activator">@p.Producto</p>
                        </div>
                        <div class="card-reveal">
                            <span class="card-title"><i class="material-icons right">close</i></span>
                            <span class="card-title">@p.Producto</span>
                            <p class="card-title">@p.Descripcion</p>
                        </div>
                    </div>
                </div>
            }
        }

    </div>

    <div class="row">
        <h5 id="empanadas">EMPANADAS <span class="precio">$ @Avanti_MVC.Models.ProductosService.ObtenerPrecioPorCategoria("empanada")</span> </h5>
        <div id="divisor" class="divider"></div>
        @{
            foreach (var p in Avanti_MVC.Models.ProductosService.ObtenerProductoPorCategoria("Empanada"))
            {

                <div class="col s6 m3 l2">
                    <div class="card">
                        <div class="card-image  waves-block waves-light">
                            <img class="activator" src="images/empanadas/@(p.Producto).jpg">
                            <span class="hide">@p.Precio</span>
                            <p class="hide">@p.Producto</p>
                            <a class="btn-floating halfway-fab waves-effect waves-light" data-id="@p.Id_producto"><i class="material-icons agregar-carrito">add</i></a>
                        </div>
                        <div class="card-content activator">
                            <span><i class="material-icons right">more_vert</i></span>
                            <p class="center activator">@p.Producto</p>
                        </div>
                        <div class="card-reveal">
                            <span class="card-title"><i class="material-icons right">close</i></span>
                            <span class="card-title">@p.Producto</span>
                            <p>@p.Descripcion</p>
                        </div>
                    </div>
                </div>
            }
        }

    </div>


    <div class="row">
        <h5 id="pizzas">PIZZAS <span class="precio">MUZZA ---> $ @Avanti_MVC.Models.ProductosService.ObtenerPrecioPorCategoria("pizza")</span> </h5>
        <div id="divisor" class="divider"></div>
        @{
            foreach (var p in Avanti_MVC.Models.ProductosService.ObtenerProductoPorCategoria("pizza"))
            {

                <div class="col s6 m3 l2">
                    <div class="card">
                        <div class="card-image waves-block waves-light">
                            <img class="activator" src="images/pizzas/@(p.Producto).jpg">
                            <span class="hide">@p.Precio</span>
                            <p class="hide">@p.Producto</p>
                            <a class="btn-floating halfway-fab waves-effect waves-light" data-id="@p.Id_producto"><i class="material-icons agregar-carrito">add</i></a>
                        </div>
                        <div class="card-content activator">
                            <span><i class="material-icons right">more_vert</i></span>
                            <p class="center activator">@p.Producto</p>
                        </div>
                        <div class="card-reveal">
                            <span class="card-title"><i class="material-icons right">close</i></span>
                            <span class="card-title">@p.Producto</span>
                            <p>@p.Descripcion</p>
